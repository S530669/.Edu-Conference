<% include adminheader %>
<head>
	<link href="/css/AdminStyling.css" type="text/css" rel="stylesheet" />
	</head>
    <body>
     <h2 align="center"> Vendor details list</h2>
     <br>
     <br>
        <div class="w3-container">
        <table class="w3-table-all w3-card-4"id="Details"  align="center">
          <tr>
            <th>Company Name</th>
            <th>city</th>
            <th>State</th>
            <th>Zip</th>
            <th>Website URL</th>
            <th>Contact Name</th>
            <th>Contact Title</th>
            <th>Email</th>
            <th>Contact Number</th>
            <th>Select</th>
          </tr>
    <% for(let i = 0; i < list.length; i++) { %>
    <tr>
      <td><%= list[i].cname %></td>
      <td><%= list[i].city %></td>
      <td><%= list[i].State %></td>
      <td><%= list[i].Zip %></td>
      <td><%= list[i].URL %></td>
      <td><%= list[i].name %></td>
      <td><%= list[i].Title %></td>
      <td><%= list[i].email %></td>
      <td><%= list[i].contact %></td>
      <td><button type="button" class="btn btn-success">Confirm</button></td>
    </tr>
    <% } %>
  </table>
  <div>
    </div> 
</div>
